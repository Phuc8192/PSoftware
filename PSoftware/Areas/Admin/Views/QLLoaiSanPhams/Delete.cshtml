@model PSoftware.Models.LoaiSanPham

@{
    ViewBag.Title = "Xóa loại sản phẩm";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPageAdmin.cshtml";
}

<h2 style="padding:30px 0px">XÓA LOẠI SẢN PHẨM</h2>
<h3>Bạn có chắc muốn xóa loại sản phẩm này?</h3>
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Về lại danh sách", "Index")
        </div>
    }
</div>

